<table border="1" style="border: 2px solid green;">
  <tr style="border: 1px solid green;">
      <td>ID</td>
      <td>Title</td>
      <td>DueDate</td>
      <td>Completed</td>
  </tr>
  <% for( var i = 0; i < allTodos.length; i++) {%>
      <tr style="border: 2px solid green;">
          <td><%= allTodos[i].id %></td>
          <td><%= allTodos[i].title %></td>
          <td><%= allTodos[i].dueDate %></td>
          <td><%= allTodos[i].completed %></td>
      </tr>
  <% } %>
</table>